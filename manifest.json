{
  "author": "igorlogius",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Shows the list of visit timestamp of the current url with web archive links",
  "icons": {
      "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "show-visits",
  "page_action": {
      "default_icon": "icon.png",
    "default_popup": "popup.html",
    "show_matches": [
      "<all_urls>"
    ]
  },
  "permissions": [
    "activeTab",
    "history"
  ],
  "version": "1.0.2"
}
