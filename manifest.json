{
  "author": "igorlogius",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Shows on hover of the page Action icon when the current url was last visited",
  "icons": {
      "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "show-last-visited-on-hover",
  "page_action": {
      "default_icon": "icon.png",
    "show_matches": [
      "<all_urls>"
    ]
  },
  "permissions": [
    "tabs",
    "history"
  ],
  "version": "1.0.2"
}
